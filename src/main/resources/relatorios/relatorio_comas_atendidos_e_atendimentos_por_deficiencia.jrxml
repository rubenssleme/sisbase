<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="relatorio_comas_atendidos_e_atendimentos_por_deficiencia" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="45772ca4-fd53-4694-9bc0-357113486941">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="param_data_inicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="param_data_termino" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select aux3.descricao, sum(aux3.total_atendidos) as total_atendidos, sum(aux3.total_atendimentos)as total_atendimentos from (
	select aux.descricao, count(distinct aux.id) as total_atendidos, 0 as total_atendimentos from(
		select def.descricao, infess.id from grupo gru
			inner join grupo_modulo_periodo grumodper
				on gru.id = grumodper.id_grupo
			inner JOIN modulo_periodo modper
				ON grumodper.id_modulo_periodo = modper.id
			inner join descricao_tipo_atendimento destipate
				on (destipate.id = gru.id_descricao_tipo_atendimento)
			inner join tipo_atendimento tip
				on (tip.id = destipate.id_tipo_atendimento)
			inner JOIN modulo mod
				ON modper.id_modulo = mod.id
			inner JOIN modulo_periodo_atendimento_grupo modperaten
				ON modper.id = modperaten.id_modulo_periodo
			inner JOIN atendimento_grupo ate
				ON ate.id = modperaten.id_atendimento_grupo
			inner join atendimento_grupo_atendimento_usuario ategruateusu
				on ate.id = ategruateusu.id_atendimento_grupo
			inner join atendimento_usuario ateusu
				on ateusu.id = ategruateusu.id_atendimento_usuario
			inner join usuario usu
				on usu.id = ateusu.id_usuario
			inner join informacao_atendimento infate
				on infate.id = ateusu.id_informacao_atendimento
			inner join atendimento_grupo_atendimento_profissional ategruatepro
				on ategruatepro.id_atendimento_grupo = ate.id
			inner join atendimento_profissional atepro
				on atepro.id = ategruatepro.id_atendimento_profissional
			inner join informacao_atendimento infatepro
				on infatepro.id = atepro.id_informacao_atendimento
			inner join informacao_essencial infess
				on infess.id = usu.id_informacao_essencial
			inner join usuario_periodo_deficiencia usuperdef
				on usu.id = usuperdef.id_usuario
			inner join periodo_deficiencia perdef
				on perdef.id = usuperdef.id_periodo_deficiencia
			inner join deficiencia def
				on def.id = perdef.id_deficiencia
		WHERE ate.status = 'NORMAL' and gru.excluido is false and infate.frequencia = 'AT' and infatepro.frequencia = 'AT' and ate.data between $P{param_data_inicio} and $P{param_data_termino} and (perdef.data_inicial_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia is null)
	union all
	select def.descricao, infess.id from atendimento_individual ateind
		left join usuario usu
			on usu.id = ateind.id_usuario
		inner join informacao_essencial infess
			on (infess.id = usu.id_informacao_essencial)
		inner join informacao_atendimento infate
			on infate.id = ateind.id_informacao_atendimento
		inner join descricao_tipo_atendimento destipate
			on (destipate.id = ateind.id_descricao_tipo_atendimento)
		inner join tipo_atendimento tip
			on (tip.id = destipate.id_tipo_atendimento)
		inner join modulo mod
			on (mod.id = ateind.id_modulo)
		inner join atendimento_individual_atendimento_profissional ateindatepro
			on ateindatepro.id_atendimento_individual = ateind.id
		inner join atendimento_profissional atepro
			on atepro.id = ateindatepro.id_atendimento_profissional
		inner join informacao_atendimento infatepro
			on infatepro.id = atepro.id_informacao_atendimento
		inner join usuario_periodo_deficiencia usuperdef
			on usu.id = usuperdef.id_usuario
		inner join periodo_deficiencia perdef
			on perdef.id = usuperdef.id_periodo_deficiencia
		inner join deficiencia def
			on def.id = perdef.id_deficiencia
	where ateind.status = 'NORMAL' and infate.frequencia = 'AT' and infatepro.frequencia = 'AT' and ateind.data between $P{param_data_inicio} and $P{param_data_termino} and (perdef.data_inicial_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia is null))aux
	group by aux.descricao
	union all
	select aux2.descricao, 0 as total_atendidos, sum(aux2.total_atendimentos) as total_atendimentos from (
		select aux.descricao, sum(aux.total_atendimentos) as total_atendimentos from(
			select ategru.id, def.descricao, count(distinct atepro.id_profissional)*count(distinct usu.id) as total_atendimentos from grupo gru
				inner join grupo_modulo_periodo grumodper
					on gru.id = grumodper.id_grupo
				inner JOIN modulo_periodo modper
					ON grumodper.id_modulo_periodo = modper.id
				inner join descricao_tipo_atendimento destipate
					on (destipate.id = gru.id_descricao_tipo_atendimento)
				inner join tipo_atendimento tip
					on (tip.id = destipate.id_tipo_atendimento)
				inner JOIN modulo mod
					ON modper.id_modulo = mod.id
				inner JOIN modulo_periodo_atendimento_grupo modperaten
					ON modper.id = modperaten.id_modulo_periodo
				inner JOIN atendimento_grupo ategru
					ON ategru.id = modperaten.id_atendimento_grupo
				inner join atendimento_grupo_atendimento_usuario ategruateusu
					on ategru.id = ategruateusu.id_atendimento_grupo
				inner join atendimento_usuario ateusu
					on ateusu.id = ategruateusu.id_atendimento_usuario
				inner join usuario usu
					on usu.id = ateusu.id_usuario
				inner join informacao_essencial infess
					on (infess.id = usu.id_informacao_essencial)
				inner join informacao_atendimento infate
					on infate.id = ateusu.id_informacao_atendimento
				inner join atendimento_grupo_atendimento_profissional ategruatepro
					on ategruatepro.id_atendimento_grupo = ategru.id
				inner join atendimento_profissional atepro
					on atepro.id = ategruatepro.id_atendimento_profissional
				inner join informacao_atendimento infatepro
					on infatepro.id = atepro.id_informacao_atendimento
				inner join usuario_periodo_deficiencia usuperdef
					on usu.id = usuperdef.id_usuario
				inner join periodo_deficiencia perdef
					on perdef.id = usuperdef.id_periodo_deficiencia
				inner join deficiencia def
					on def.id = perdef.id_deficiencia
			WHERE ategru.status = 'NORMAL' and gru.excluido is false and infate.frequencia = 'AT' and infatepro.frequencia = 'AT' and ategru.data  between $P{param_data_inicio} and $P{param_data_termino} and (perdef.data_inicial_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia is null)
			group by ategru.id, def.descricao)aux
		group by aux.descricao
		union all
		select aux.descricao, sum(aux.total_atendimentos) as total_atendimentos from(
			select ateind.id, def.descricao, count(distinct atepro.id_profissional)*count(distinct infess.id) as total_atendimentos from atendimento_individual ateind
				inner join usuario usu
					on usu.id = ateind.id_usuario
				inner join informacao_atendimento infate
					on infate.id = ateind.id_informacao_atendimento
				inner join informacao_essencial infess
					on (infess.id = usu.id_informacao_essencial)
				inner join atendimento_individual_atendimento_profissional ateindatepro
					on ateindatepro.id_atendimento_individual = ateind.id
				inner join atendimento_profissional atepro
					on atepro.id = ateindatepro.id_atendimento_profissional
				inner join informacao_atendimento infatepro
					on infatepro.id = atepro.id_informacao_atendimento
				inner join descricao_tipo_atendimento destipate
					on (destipate.id = ateind.id_descricao_tipo_atendimento)
				inner join tipo_atendimento tip
					on (tip.id = destipate.id_tipo_atendimento)
				inner join modulo mod
					on (mod.id = ateind.id_modulo)
				inner join usuario_periodo_deficiencia usuperdef
					on usu.id = usuperdef.id_usuario
				inner join periodo_deficiencia perdef
					on perdef.id = usuperdef.id_periodo_deficiencia
				inner join deficiencia def
					on def.id = perdef.id_deficiencia
			where ateind.status = 'NORMAL' and infate.frequencia = 'AT' and infatepro.frequencia = 'AT'  and ateind.data between $P{param_data_inicio} and $P{param_data_termino} and (perdef.data_inicial_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia between $P{param_data_inicio} and $P{param_data_termino} or perdef.data_final_vigencia is null)
			group by ateind.id, def.descricao)aux
		group by aux.descricao)aux2
	group by aux2.descricao)aux3
group by aux3.descricao
order by sum(aux3.total_atendidos) desc;]]>
	</queryString>
